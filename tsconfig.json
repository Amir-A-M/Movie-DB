{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@services/*": [
        "./src/services/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.jsx",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
  ],
  "exclude": [
    "node_modules",
    "dist",
    "__tests__",
    ".expo",
    ".idea",
    ".vscode"
  ]
}